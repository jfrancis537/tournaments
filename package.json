{
  "name": "tournament-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite serve ./client",
    "dev:backend": "TSX_TSCONFIG_PATH=./server/tsconfig.json tsx --env-file server/.env.dev --watch ./server/src/index.ts",
    "dev": "concurrently 'yarn:dev:frontend' 'yarn:dev:backend'",
    "tsgen": "tcm client/src",
    "build:frontend": "tsc --project ./client/tsconfig.json && vite build ./client",
    "build:backend": "rm -rf ./server/dist && tsc --project ./server/tsconfig.json && tsc-alias -p ./server/tsconfig.json && rollup --config ./server/rollup.config.mjs",
    "build": "yarn build:frontend && yarn build:backend",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview ./client"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/joy": "^5.0.0-beta.19",
    "@mui/material": "^5.15.1",
    "brackets-manager": "^1.6.4",
    "brackets-memory-db": "^1.0.4",
    "brackets-model": "^1.5.0",
    "brackets-viewer": "^1.6.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "luxon": "^3.4.4",
    "nodemailer": "^6.9.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rfdc": "^1.3.0",
    "rollup-plugin-html": "^0.2.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.1",
    "wouter": "^2.12.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/luxon": "^3.3.7",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "http-proxy-middleware": "^2.0.6",
    "rollup": "^4.9.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.2",
    "typed-css-modules": "^0.8.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.2"
  }
}
